                        {
                            <li><a href="#tabs-1">Traslado</a></li>
                        }
                        @if (AtividadeModel.AssistenciaViagem.Count > 0)
                        {
                            <li><a href="#tabs-2">Assistencia Viagem</a></li>
                        }
                        @if (AtividadeModel.Ingresso.Count > 0)
                        {
                            <li><a href="#tabs-3">Ingresso</a></li>
                        }
                        @if (AtividadeModel.Passeio.Count > 0)
                        {
                            <li><a href="#tabs-4">Passeio</a></li>
                        }
                        @if (AtividadeModel.Restaurante.Count > 0)
                        {
                            <li><a href="#tabs-5">Restaurante</a></li>
                        }
                        @if (AtividadeModel.PacoteAtividade.Count > 0)
                        {
                            <li><a href="#tabs-6">Pacote </a></li>
                        }

                                                    <div class="pacote-detalhes-apartirde">Total: R$ @TransferModel.Tarifas[0].Value.Split('|')[1]</div>
                                                    <input type="hidden" id="hdnValorTotal_@TransferModel.CodigoAtividade" value="@TransferModel.Tarifas[0].Value.Split('|').First()" />                                            
                                                    <input type="hidden" id="hdnCustoTotal_@TransferModel.CodigoAtividade" value="@TransferModel.Tarifas[0].Value.Split('|').Last()" />      
                                                    }
                                                    else
                                                    {
                                                    <div class="pacote-detalhes-apartirde">Total: R$ @TransferModel.Tarifas[0].Value</div>
                                                    <input type="hidden" id="hdnValorTotal_@TransferModel.CodigoAtividade" value="@TransferModel.Tarifas[0].Value" />                                            
                                                    <input type="hidden" id="hdnCustoTotal_@TransferModel.CodigoAtividade" value="@TransferModel.Tarifas[0].Value" />      
                                                    }
                                                }

                                            </div>
                                            @if (ViewBag.IsMontePacote == null)
                                            {
                                                if (@TransferModel.CodigoStatusVenda == 0)
                                                {//SOLICITAR
                                                <a  style="margin-left:30px !important;" class="button_solicitar"  onclick="AdicionarAtividadeTransfer(@TransferModel.CodigoAtividade, true)">Solicitar</a>
                                                }
                                                if (@TransferModel.CodigoStatusVenda == 1)
                                                {
                                                <a class="button_comprar_atividade"   onclick="AdicionarAtividadeTransfer(@TransferModel.CodigoAtividade, true)">Comprar</a>
                                                }

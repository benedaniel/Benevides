                                                <a style="margin-left:30px !important;" class="button_lotado"  onclick="AdicionarAtividadeTransfer(@TransferModel.CodigoAtividade, true)">Lotado</a>
                                                }
                                                <div>
                                                    <img style="height: 31px; margin-left: 2px;  cursor:pointer" src="../Images/adicionar-carrinho.png" onclick="AdicionarAtividadeTransfer(@TransferModel.CodigoAtividade, false)"/>
                                                </div>
                                        
                                                <br />
                                                <a onclick="VerPrecoParcelas(@TransferModel.CodigoAtividade, 'Transfer', null)" class="btnTesteAzul">Formas de pagamento</a>
                                                <div class="@TransferModel.CodigoAtividade-Parcelas-Transfer tbsParcela">
                                                    <div>
                                                        <img src="~/Images/loading_transparent.gif" />
                                                    </div>
                                                    <a onclick="FecharPrecoParcelas(@TransferModel.CodigoAtividade,'Transfer')"><span class="fecharDetalhePreco">fechar</span></a>
                                                </div>
                                        

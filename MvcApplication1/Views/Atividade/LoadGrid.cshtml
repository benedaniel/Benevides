                                <div class="pacotes-detalhes-hospedagem-p">
                                    <div class="pacotes-detalhes-hospedagem-p-l">
                                        <div class="pacotes-detalhes-hospedagem-texto-1">@Request.Form["Destinos"]</div>
                                        <div class="pacotes-detalhes-hospedagem-texto-2">@TransferModel.NomeAtividade</div>
                                        <div class="pacotes-detalhes-hospedagem-texto-3">@TransferModel.DescricaoAtividade</div>

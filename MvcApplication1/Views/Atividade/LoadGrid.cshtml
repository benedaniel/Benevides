                                            <div class="pacotes-detalhes-hotel-1">
                                                @foreach (FT.Web.Model.Models.TarifaModel TarifaModel in TransferModel.TarifasItem)
                                                {
                                                    @Html.DropDownList("ddl" + @TransferModel.CodigoAtividade, (IEnumerable<SelectListItem>)@TransferModel.Tarifas, new { @class = "SelectAtividadeTarifa", @onchange = "DataTarifaChange(" + @TransferModel.CodigoAtividade + ")" })
